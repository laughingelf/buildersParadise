<h2>Welcome to Builders Paradise</h2>


{{#if posts}}

{{#each posts}}
<div>

    <h4><a href="/post/post-details/{{id}}">{{title}}</a></h4>
    <p>{{description}}</p>
    <p>{{imageUrl}}</p>
    <h5>@{{owner.username}}</h5>
    <br>

    <hr>

</div>
{{/each}}


{{else}}

<h2>No Posts Yet</h2>


{{/if}}